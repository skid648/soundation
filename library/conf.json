{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["library/src", "package.json"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "referenceTitle": "Soundation"
  },
  "opts": {
    "destination": "./examples/image_translation/docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./development/jsdoc-template"
  }
}